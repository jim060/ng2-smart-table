{
  "extends": "../../tslint.json",
  "rules": {
    "no-trailing-whitespace": false,
    "no-string-literal": false,
    "quotemark": [
      false,
      "single"
    ],
    "max-line-length": [
      true,
      240
    ],
    "object-literal-key-quotes": [
      false,
      "as-needed"
    ],
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
     "element",
      "lib",
      "kebab-case"
    ]
  }
}
